{
  "name": "psycho-help",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "dependencies": {
    "@ant-design/compatible": "5.1.4",
    "@ant-design/icons": "6.1.0",
    "antd": "5.27.4",
    "antd-mask-input": "^2.0.7",
    "axios": "^1.12.2",
    "dayjs": "^1.11.10",
    "dayjs-plugin-utc": "0.1.2",
    "react": "^18.3.0",
    "react-dom": "^18.3.0",
    "react-router-dom": "7.9.3",
    "swiper": "^12.0.2",
    "zustand": "^5.0.8"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint",
    "lint:fix": "eslint --fix .",
    "lint:staged": "eslint --ext .ts,.tsx $(git diff --cached --name-only | grep -E '\\.(ts|tsx)$')",
    "lint:staged:fix": "files=$(git diff --cached --name-only | grep -E '\\.(ts|tsx)$'); [ -n \"$files\" ] && eslint --fix $files || exit 0",
    "lint:diff": "eslint $(git diff --name-only HEAD~1 | grep -E '\\.(ts|tsx)$')",
    "lint:diff:fix": "eslint --fix $(git diff --name-only HEAD~1 | grep -E '\\.(ts|tsx)$')",
    "format": "prettier --write",
    "format:staged": "prettier --write $(git diff --cached --name-only | grep -E '\\.(ts|tsx|js|jsx|json|css|scss|md)$')",
    "format:diff": "prettier --write $(git diff --name-only HEAD~1 | grep -E '\\.(ts|tsx|js|jsx|json|css|scss|md)$')",
    "prepare": "husky install",
    "preview": "vite preview"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/css-modules": "^1.0.5",
    "@types/node": "24.6.2",
    "@types/react": "^18.3.0",
    "@types/react-dom": "^18.3.0",
    "@types/swiper": "^5.4.3",
    "@vitejs/plugin-react": "^5.0.4",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^6.1.0",
    "eslint-plugin-react-refresh": "^0.4.23",
    "eslint-plugin-unused-imports": "^4.2.0",
    "husky": "^9.1.6",
    "jsdom": "^27.0.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9",
    "vite-plugin-svgr": "^4.5.0",
    "vitest": "^3.2.4"
  }
}
